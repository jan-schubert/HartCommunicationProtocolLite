<Application x:Class="HartAnalyzer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:HartAnalyzer.Converters"
             xmlns:properties="clr-namespace:HartAnalyzer.Properties">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/HartAnalyzer.Design;component/MainDesign.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <converters:ByteArrayToStringConverter x:Key="ByteArrayToStringConverter" />
             
             <properties:Settings x:Key="Settings" />
             
         </ResourceDictionary>
    </Application.Resources>
</Application>
